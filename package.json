{
  "name": "psbhackandchange",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "install:backend": "npm install --prefix backend",
    "install:front": "npm install --prefix front",
    "install:all": "npm run install:backend && npm run install:front",
    "dev:backend": "npm run dev --prefix backend",
    "dev:front": "npm run dev --prefix front",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:front\"",
    "start:backend": "npm start --prefix backend",
    "build:front": "npm run build --prefix front"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
